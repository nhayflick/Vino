<script>

  var queries = <%= @queries.to_json.html_safe%>;
  var $navContent = $("#nav-content");
  var $main = $(".main");
  var $content = $(".content");

  $(function(){

    //Browser compatibility test for MP4 video

    var v = document.createElement("video");
    if (v.canPlayType("video/mp4") == '') {
      window.alert("Your current browser does not support HTML5 video. For the full Vino experience, please use the latest version of Chrome or Safari.")
    }

    window.VI.initialize(
      queries,
      $navContent,
      $main,
      $content
    )
  });

</script>